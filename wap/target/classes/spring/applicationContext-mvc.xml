<?xml version="1.0" encoding="UTF-8"?><beans default-lazy-init="false"       xmlns="http://www.springframework.org/schema/beans"       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"       xmlns:mvc="http://www.springframework.org/schema/mvc"       xsi:schemaLocation="http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.2.xsd	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd	">    <mvc:annotation-driven />    <mvc:interceptors>        <mvc:interceptor>            <!--拦截管理请求-->             <!--<mvc:mapping path="/manager/**" /> -->            <!-- 不拦截的url -->             <mvc:mapping path="/**"/>            <mvc:exclude-mapping path="/"/>       	   <mvc:exclude-mapping path="/indexShop"/>             <mvc:exclude-mapping path="/ajaxIndexShopInfo"/>             <mvc:exclude-mapping path="/queryNewestNoticeInfo"/>             <mvc:exclude-mapping path="/login/**"/>             <mvc:exclude-mapping path="/payCallBack/**"/>             <!--  <mvc:exclude-mapping path="/pick/**"/>   -->       		<mvc:exclude-mapping path="/shop/**"/>        		<mvc:exclude-mapping path="/companyConsult/**"/>        		<mvc:exclude-mapping path="/nearbyShop/**"/>            <!--             <mvc:exclude-mapping path="/customerIndex/**"/>            <mvc:exclude-mapping path="/productType/**"/>  -->                        <mvc:exclude-mapping path="/master/**"/>            <mvc:exclude-mapping path="/personalCenter/goSaveShopInfo"/>            <mvc:exclude-mapping path="/generateProductImage"/>             <mvc:exclude-mapping path="/register/**"/>             <mvc:exclude-mapping path="/address/**"/>             <mvc:exclude-mapping path="/images/**"/>             <mvc:exclude-mapping path="/css/**"/>             <mvc:exclude-mapping path="/js/**"/>             <mvc:exclude-mapping path="/js/dist/**"/>             <mvc:exclude-mapping path="/jsmin/**"/>             <mvc:exclude-mapping path="/iconfont/**"/>             <mvc:exclude-mapping path="/Font-Awesome/**"/>             <mvc:exclude-mapping path="/music/**"/>             <mvc:exclude-mapping path="/layer-mobile/**"/>             <mvc:exclude-mapping path="/bootstrap/**"/>             <mvc:exclude-mapping path="/bootstrap-switch-master/**"/>             <mvc:exclude-mapping path="/weixin/**"/>             <mvc:exclude-mapping path="/weixinPay/**"/>             <mvc:exclude-mapping path="/openedRegional/**"/>                                     <mvc:exclude-mapping path="/master/index"/>             <mvc:exclude-mapping path="/customerIndex/goServiceDetail"/>             <mvc:exclude-mapping path="/customerIndex/generateImage"/>             <!--登录拦截器-->            <bean id="LoginInterceptor" class="com.linkon.hgl.web.interceptors.LoginInterceptor" />        </mvc:interceptor>    </mvc:interceptors></beans>